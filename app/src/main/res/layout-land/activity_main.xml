<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/tapLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MainActivity">

    <VideoView
        android:id="@+id/video_View"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"/>
    <TextView
        android:id="@+id/textView1"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="10dp"
        android:gravity="right"
        android:textSize="15sp"
        android:layout_marginTop="5dp"
        android:text="Sunglass :" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="15dp"
        android:gravity="right"
        android:textSize="15sp"
        android:text="Tops :" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:layout_marginTop="15dp"
        android:gravity="right"
        android:textSize="15sp"
        android:text="Jacket :" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="15dp"
        android:gravity="right"
        android:textSize="15sp"
        android:text="Shirts :" />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="15dp"
        android:gravity="right"
        android:textSize="15sp"
        android:text="Dress :" />
    <TextView
        android:id="@+id/textView7"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="15dp"
        android:gravity="right"
        android:textSize="15sp"
        android:text="Skirt :" />
    <TextView
        android:id="@+id/textView8"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="20dp"
        android:gravity="right"
        android:textSize="15sp"
        android:text="Bag :" />
    <TextView
        android:id="@+id/textView9"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView8"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="15dp"
        android:gravity="right"
        android:textSize="15sp"
        android:text="Globe :" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="15dp"
        android:gravity="right"
        android:textSize="15sp"
        android:text="Shoes :" />

    <TextView
        android:id="@+id/counter1"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/textView1"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="3dp"
        android:textSize="18sp"
        android:text="15sp" />
    <TextView
        android:id="@+id/counter2"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/counter1"
        android:layout_below="@+id/counter1"
        android:layout_marginTop="11dp"
        android:textSize="18sp"
        android:text="15sp" />
    <TextView
        android:id="@+id/counter3"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/counter2"
        android:layout_alignLeft="@+id/counter2"
        android:layout_marginTop="11dp"
        android:textSize="18sp"
        android:text="15" />
    <TextView
        android:id="@+id/counter4"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/counter3"
        android:layout_alignLeft="@+id/counter3"
        android:layout_marginTop="11dp"
        android:textSize="18sp"
        android:text="15sp" />
    <TextView
        android:id="@+id/counter5"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/counter4"
        android:layout_alignLeft="@+id/counter4"
        android:layout_marginTop="10dp"
        android:textSize="18sp"
        android:text="20" />
    <TextView
        android:id="@+id/counter6"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/counter5"
        android:layout_alignLeft="@+id/counter5"
        android:layout_marginTop="13dp"
        android:textSize="18sp"
        android:text="20" />
    <TextView
        android:id="@+id/counter7"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/counter6"
        android:layout_alignLeft="@+id/counter6"
        android:layout_marginTop="14dp"
        android:textSize="18sp"
        android:text="20" />
    <TextView
        android:id="@+id/counter8"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/counter7"
        android:layout_alignLeft="@+id/counter7"
        android:layout_marginTop="12dp"
        android:textSize="18sp"
        android:text="20" />
    <TextView
        android:id="@+id/counter9"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/counter8"
        android:layout_alignLeft="@+id/counter8"
        android:layout_marginTop="12dp"
        android:textSize="18sp"
        android:text="20" />
    <Button
        android:id="@+id/button1"
        android:layout_width="70dp"
        android:layout_height="60dp"
        android:background="@null"
        android:layout_centerHorizontal="true" />
    <Button
        android:id="@+id/button2"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_below="@+id/button1"
        android:background="@null"
        android:layout_marginLeft="250dp" />
    <Button
        android:id="@+id/button3"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_below="@+id/button1"
        android:layout_alignLeft="@+id/button2"
        android:background="@null"
        android:layout_marginLeft="90dp" />
    <Button
        android:id="@+id/button4"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_below="@+id/button2"
        android:background="@null"
        android:layout_alignLeft="@+id/button2" />
    <Button
        android:id="@+id/button5"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_below="@+id/button2"
        android:layout_alignLeft="@+id/button4"
        android:background="@null"
        android:layout_marginLeft="90dp" />
    <Button
        android:id="@+id/button6"
        android:layout_width="85dp"
        android:layout_height="80dp"
        android:layout_below="@id/button5"
        android:background="@null"
        android:layout_centerHorizontal="true" />
    <Button
        android:id="@+id/button7"
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:layout_below="@+id/button4"
        android:background="@null"
        android:layout_alignRight="@+id/button6"
        android:layout_marginRight="85dp" />
    <Button
        android:id="@+id/button8"
        android:layout_width="50dp"
        android:layout_height="90dp"
        android:layout_below="@+id/button4"
        android:layout_alignLeft="@+id/button6"
        android:background="@null"
        android:layout_marginLeft="85dp" />
    <Button
        android:id="@+id/button9"
        android:layout_width="85dp"
        android:layout_height="70dp"
        android:layout_below="@id/button6"
        android:background="@null"
        android:layout_centerHorizontal="true" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/imageButton"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:background="@null"
        android:src="@drawable/ic_check_circle_light_blue_a400_48dp" />



</RelativeLayout>